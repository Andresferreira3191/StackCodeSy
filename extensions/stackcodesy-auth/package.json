{
  "name": "stackcodesy-auth",
  "displayName": "StackCodeSy Authentication",
  "description": "Custom authentication provider for StackCodeSy platform",
  "version": "1.0.0",
  "publisher": "stackcodesy",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "authentication": [
      {
        "id": "stackcodesy",
        "label": "StackCodeSy"
      }
    ],
    "commands": [
      {
        "command": "stackcodesy.auth.login",
        "title": "Sign in to StackCodeSy"
      },
      {
        "command": "stackcodesy.auth.logout",
        "title": "Sign out of StackCodeSy"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^6.x",
    "@typescript-eslint/parser": "^6.x",
    "eslint": "^8.x",
    "typescript": "^5.3.0",
    "@vscode/vsce": "^2.22.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
